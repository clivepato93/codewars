// Arrays
// Fundamentals

function dominator(arr) {
   //code me
  let obj = {};
  for(let num of arr){
    obj[num] = (obj[num]|| 0) +1;
  }
  for(let n in obj) {
    if(obj[n] > arr.length/2){
      return +n
    }
  }
  return -1
}
